{
  "name": "acf-dimensions",
  "title": "ACF Dimensions",
  "version": "1.0.4",
  "license": "GPL-2.0+",
  "main_file": "acf-dimensions.php",
  "repository": {
    "type": "git",
    "url": "https://github.com/ernilambar/acf-dimensions"
  },
  "scripts": {
    "dev": "webpack --watch",
    "build": "webpack",
    "prod": "NODE_ENV=production webpack",
    "eslint": "eslint --quiet .",
    "eslint:fix": "eslint --quiet --fix .",
    "format": "prettier --write \"src/**/*.scss\"",
    "preinstall": "npx only-allow pnpm",
    "pot": "wpi18n makepot --domain-path=languages --exclude=vendor,deploy,node_modules",
    "textdomain": "wpi18n addtextdomain --exclude=vendor,deploy,node_modules",
    "version": "easy-replace-in-files",
    "deploy": "shx rm -rf deploy/ && shx mkdir deploy && copy-files-from-to && cd deploy/ && cross-var shx mv temp $npm_package_name && cross-var bestzip ../$npm_package_name.zip * && cd .. && cross-var shx mv $npm_package_name.zip deploy/"
  },
  "browserslist": "> 1%, not dead",
  "prettier": "@wordpress/prettier-config",
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@wordpress/eslint-plugin": "^12.9.0",
    "@wordpress/prettier-config": "^1.4.0",
    "babel-loader": "^8.2.5",
    "bestzip": "^2.2.1",
    "clean-webpack-plugin": "^4.0.0",
    "copy-files-from-to": "^3.2.2",
    "cross-var": "^1.1.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.1.0",
    "easy-replace-in-files": "^1.0.3",
    "eslint": "^8.23.1",
    "mini-css-extract-plugin": "^2.6.1",
    "node-wp-i18n": "^1.2.6",
    "postcss": "^8.4.16",
    "postcss-loader": "^7.0.1",
    "postcss-preset-env": "^7.8.2",
    "prettier": "^2.7.1",
    "sass": "^1.54.9",
    "sass-loader": "^13.0.2",
    "shx": "^0.3.4",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.6",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
